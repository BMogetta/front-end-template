{
  "name": "front-end-template",
  "description": "Using pnpm-ts-vite-vitest-eslint-prettier-husky-amplify",
  "version": "1.1.0",
  "author": {
    "name": "Bruno Mogetta <brunomogetta@gmail.com>",
    "url": "https://github.com/BMogetta"
  },
  "maintainers": [
    "Leandro Patriarca <patriarcaleandro@gmail.com>",
    "Pedro Bustos <pebustos.135@gmail.com>"
  ],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/BMogetta/front-end-template.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc --project tsconfig.build.json && vite build",
    "serve": "vite preview",
    "test": "vitest --run --coverage --passWithNoTests",
    "test:watch": "vitest --passWithNoTests",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "pnpm lint:format && pnpm lint:fix ",
    "type-check": "tsc",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "6.4.2",
    "i18next": "22.0.6",
    "react-i18next": "12.0.0",
    "i18next-browser-languagedetector": "7.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitejs/plugin-react": "^2.2.0",
    "@vitest/coverage-c8": "^0.25.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-no-unsanitized": "4.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-security": "0.1.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "jsdom": "^20.0.2",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vite-tsconfig-paths": "3.5.2",
    "vitest": "^0.25.1",
    "husky": "^8.0.0",
    "lint-staged": "13.0.4"
  },
  "license": "MIT",
  "private": true
}
